<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>freeform</id>
    <version>0.0.1</version>
    <title>Freeform CSS</title>
    <authors>The Freeman Company, The Bootstrap Authors, Twitter Inc.</authors>
    <owners>freeman</owners>
